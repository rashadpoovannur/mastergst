{
    "name": "rashadpoovannur/mastergst",
    "description": "MasterGst",
    "keywords": [
        "rashadpoovannur",
        "mastergst"
    ],
    "homepage": "https://github.com/rashadpoovannur/mastergst",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Rashad",
            "email": "rashadrashad607@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Rashadpoovannur\\Mastergst\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rashadpoovannur\\Mastergst\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rashadpoovannur\\Mastergst\\MastergstServiceProvider"
            ],
            "aliases": {
                "Mastergst": "Rashadpoovannur\\Mastergst\\MastergstFacade"
            }
        }
    }
}
